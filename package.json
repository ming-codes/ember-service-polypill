{
  "name": "ember-service-polyfill",
  "version": "1.0.2",
  "description": "ember-service-polyfill is an Ember CLI addon to polyfill Ember.Service and Ember.inject to Ember versions that has no support for services (<= 1.8)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test"
  },
  "repository": "https://github.com/ming-codes/ember-service-polyfill",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Ming Liu",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-ember-hbs-template-compiler": "^1.6.1",
    "ember-cli": "2.11.1",
    "ember-cli-6to5": "^3.0.0",
    "ember-cli-app-version": "2.0.1",
    "ember-cli-content-security-policy": "0.6.0",
    "ember-cli-dependency-checker": "1.3.0",
    "ember-cli-ic-ajax": "1.0.0",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "3.1.1",
    "ember-cli-uglify": "1.2.0",
    "ember-data": "2.11.1",
    "ember-export-application-global": "^1.0.2",
    "express": "^4.8.5",
    "glob": "^7.1.1"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
